{
  "expo": {
    "name": "expo-test",
    "slug": "expo-test",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ahmeddalton.expotest"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ahmeddalton.expotest"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],[
        "expo-font",
        {
        "fonts": ["node_modules/@expo-google-fonts/readex-pro/ReadexPro_400Regular.ttf", 
            "node_modules/@expo-google-fonts/readex-pro/ReadexPro_500Medium.ttf",
            "node_modules/@expo-google-fonts/readex-pro/ReadexPro_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/readex-pro/ReadexPro_700Bold.ttf"
          ]

        }
     ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "15a6f3db-96fb-4cd0-ae51-f560b5a961e1"
      }
    }
  }
}
